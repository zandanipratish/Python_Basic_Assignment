    "'hello'\n",
    "-87.8\n",
    "- \n",
    "/ \n",
    "+\n",
    "6 \n",
    "\n",
    "Ans : \n",
    "    Values :- 'hello', -87.8, 6\n",
    "    expressions :- * , - , / , + \n"
   ]
  },
